<h1> Chitter  </h1>

<br>
<form action='/users/new' method='get'>
  <input type='submit' value='Sign up'>
</form>
<br>

<form action='/sessions/new' method='get'>
  <input type='submit' value='Log in'>
</form>

<ul id='peeps'>
  <% @peeps.each do |peep| %>
    <li>
      <%= peep.content %>
      <br>
      Posted on <%= (peep.created_at)%>
    </li>
  <% end %>
</ul>